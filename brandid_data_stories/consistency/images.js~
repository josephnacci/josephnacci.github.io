function loadImages(imgArr, targetId){
    for(var i=0; i< imgArr.length; i++) {
        console.log(imgArr[i]);
        var img = new Image();
	img.src = imgArr[i];
        document.getElementById('output').appendChild(img);
    }
}

loadImages(images);